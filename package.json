{
  "name": "craigponent-library",
  "version": "0.1.0",
  "private": true,
  "description": "React Component Library for UI consistency between repositories",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=10.9.0"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:coverage": "yarn test --coverage",
    "test:watch": "react-scripts test --env=jsdom",
    "x": "node ./scripts/buildDist",
    "build": "node ./scripts/buildDist && rollup -c",
    "build:web": "docz build",
    "watch": "rollup -c -w",
    "start": "docz dev",
    "dev": "yarn start"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "colors": "^1.3.3",
    "cross-env": "^5.1.4",
    "css-loader": "^3.2.0",
    "docz": "^2.0.0-rc.41",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^2.1.2",
    "node-sass": "^4.12.0",
    "prettier": "^1.15.2",
    "react": "^16.9.0",
    "react-component-component": "^1.2.1",
    "react-dom": "^16.9.0",
    "react-dropzone": "^8.1.0",
    "react-scripts": "^1.1.4",
    "react-swipeable": "^5.4.0",
    "react-transition-group": "^2.5.3",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-url": "^1.4.0",
    "sass-loader": "^7.3.1",
    "scheduler": "^0.15.0",
    "stylelint": "^9.8.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-declaration-use-variable": "^1.7.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.4.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "uniqid": "^5.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/icons/**",
      "!**/utils/**",
      "!src/index.js",
      "!src/setupTests.js"
    ]
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "lohook": "^0.0.2"
  }
}
